{
  "name": "cfn-hooks",
  "private": true,
  "version": "0.0.0",
  "bin": {
    "cfn-hooks": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "NODE_OPTIONS=--experimental-vm-modules POWERTOOLS_DEV=true jest --silent",
    "test:chatbot": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --coverageReporters=text",
    "cdk": "cdk",
    "lint": "eslint .",
    "format": "eslint --fix && prettier --write \"**/*.{ts,js,json,yml,yaml}\"",
    "generate-deployment-assets": "tsx scripts/generate-deployment-assets.ts"
  },
  "dependencies": {
    "@trust-stack/schema": "workspace:*",
    "@trust-stack/utils": "workspace:*",
    "@aws-lambda-powertools/logger": "catalog:",
    "@aws-sdk/client-cloudformation": "catalog:",
    "@aws-sdk/client-ec2": "catalog:",
    "@aws-sdk/client-iam": "catalog:",
    "@aws-sdk/client-lambda": "catalog:",
    "@aws-sdk/client-s3": "catalog:",
    "@aws-sdk/client-securityhub": "catalog:",
    "@aws-sdk/client-sns": "catalog:",
    "@aws-sdk/client-ssm": "catalog:",
    "@inquirer/prompts": "catalog:",
    "@middy/core": "catalog:",
    "aws-cdk-lib": "catalog:",
    "cfn-response": "^1.0.1",
    "constructs": "catalog:",
    "ipaddr.js": "^2.2.0",
    "js-yaml": "catalog:",
    "yargs": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@jest/globals": "catalog:",
    "@swc/core": "catalog:",
    "@swc/jest": "catalog:",
    "@types/aws-lambda": "catalog:",
    "@types/jest": "catalog:",
    "@types/js-yaml": "catalog:",
    "@types/node": "catalog:",
    "@types/yargs": "catalog:",
    "aws-cdk": "catalog:",
    "dedent": "catalog:",
    "dotenv": "catalog:",
    "esbuild": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-n": "catalog:",
    "globals": "catalog:",
    "immer": "catalog:",
    "jest": "catalog:",
    "prettier": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  }
