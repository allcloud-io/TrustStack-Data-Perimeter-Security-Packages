{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyGetDownloadUrlForLayerExceptAllowed",
      "Effect": "Deny",
      "Action": [
        "ecr:GetDownloadUrlForLayer"
      ],
      "Resource": "*",
      "Condition": {
        "StringNotLike": {
          "aws:PrincipalARN": [
            "arn:aws:iam::*:role/AdminRole",
            "arn:aws:iam::*:role/DeploymentRole",
            "arn:aws:iam::*:role/ECRAccessRole"
          ]
        }
      }
    }
  ]
}